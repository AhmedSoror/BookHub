{
  "expo": {
    "name": "Book Hub",
    "slug": "BookHub",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.GUC.BookHub"
    },
    "ios": {
      "supportsTablet": true
    },
    "description": "BookHub is an application where users can borrow books from each other in a smooth way"
  }
}